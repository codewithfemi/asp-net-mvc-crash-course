@model List<BlogListViewModel>

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Ratings</th>
            <th scope="col">Stars</th>
        </tr>
    </thead>
    <tbody>
        @{
            var count = 1;
            foreach (var blog in Model)
            {
                <tr>
                    <th scope="row">@count</th>
                    <td>@blog.Name</td>
                    <td>@blog.Ratings</td>
                    <td>@blog.GetStarRatings()</td>
                </tr>

                count++;
            }
        }
    </tbody>
</table>